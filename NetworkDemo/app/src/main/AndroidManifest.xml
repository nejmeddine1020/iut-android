<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="network.iut.org.networkdemo">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".socket.SocketActivity">
        </activity>
        <activity android:name=".url.connection.HttpURLConnectionActivity">
        </activity>
        <activity android:name=".json.JSONActivity">
        </activity>
        <activity android:name=".xml.XMLActivity">
        </activity>
    </application>

</manifest>
